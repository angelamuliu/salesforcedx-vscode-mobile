{
  "name": "offline-onboarding",
  "displayName": "Offline Onboarding",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "com.khawkins.sandbox.starterkit.wizard",
        "title": "Offline Starter Kit: Configuration Wizard"
      },
      {
        "command": "com.khawkins.sandbox.starterkit.configureProject",
        "title": "Offline Starter Kit: Create or Open Project"
      },
      {
        "command": "com.khawkins.sandbox.starterkit.deployProject",
        "title": "Offline Starter Kit: Deploy Project to Org"
      },
      {
        "command": "com.khawkins.sandbox.starterkit.landingPage",
        "title": "Offline Starter Kit: Configure Landing Page"
      },
      {
        "command": "com.khawkins.sandbox.starterkit.sObjectSetup",
        "title": "Offline Starter Kit: LWC and Quick Action Setup"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.3.0"
  }
}
